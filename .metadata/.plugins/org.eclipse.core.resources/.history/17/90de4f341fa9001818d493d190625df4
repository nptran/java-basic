package baitapso1;

import java.util.Scanner;

public class Bai18 {
	
	private static Scanner scanner = new Scanner(System.in);
	
	private static void inputArray(int size, int[] arr) {
		for (int i=0; i<size; i++) {
			arr[i] = scanner.nextInt();
		}
	}
	
	private static String reversibleArray(int size, int[] arr) {
		inputArray(size, arr);
		
		for (int i=0; i<size; i++) {
			if (arr[i] != arr[size-i-1])
				return "Mảng a không phải mảng đối xứng";
		}
		
		return "Mảng a là mảng đối xứng";
	}
	
	public static void main(String[] args) {
		int n = scanner.nextInt();
		int[] arr = new int[n];
		
		reversibleArray(n, arr);
	}
}
