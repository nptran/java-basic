package com.java.lesson2;
import java.util.Scanner;

public class Ex3 {
	
	private static Scanner sc;
	
	public static void timMax(){
		System.out.println("Enter indexs of array:");
		int n = sc.nextInt();
		System.out.println("Enter values of array:");
		int a[] = new int[n];
			for(int i= 0; i<= n; i++){
				a[i] = sc.nextInt();
				System.out.println(a[i]);
			}
		
	}
	

}
