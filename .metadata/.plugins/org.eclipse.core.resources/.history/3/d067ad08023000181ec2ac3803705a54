		//Ex1.menu();
		//Ex2.basicCalculator();		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter number of elements:");
		int n = sc.nextInt();
		System.out.println("Enter values:");
		int a[] = new int[n];
		for(int i = 0; i <n; i++ ){
				
				a[i] = sc.nextInt();
				
			}
		sc.close();	

		Ex3.findMax(a, n);
	}
}

