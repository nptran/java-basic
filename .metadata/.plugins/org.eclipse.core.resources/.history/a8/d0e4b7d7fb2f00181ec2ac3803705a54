package com.java.lesson2;
import java.util.Scanner;

public class Ex3 {
		
	private static Scanner sc;

	public static void timMax(){
		
		sc = new Scanner(System.in);
		int n = 100;
		System.out.println("Enter values of array:");
		int a[] = new int[n];
			for(int i= 0; i<= n; i++){
				a[i] = sc.nextInt();
				System.out.println(a[i]);
			}
		
	}
	

}
