package baitapso1;

import java.util.Scanner;

public class Bai17 {
	private static Scanner scanner = new Scanner(System.in);
	
	private static int formula(int k, int n) {
		if (k==0 | k==n) {
			return 1;
		}
		else {
			return formula(k, n-1) + formula(k-1, n-1);
		}
	}
	
	public static void main(String[] args) {
		int n = scanner.nextInt();
		
		for (int i=0; i<=n; i++) {
			for (int j=0; j<=i; j++) {
				System.out.println(formula(i, n));				
			}
			System.out.println();
		}
	}
}
