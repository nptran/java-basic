import java.util.*;
import java.io.*;

class Test5 {

	static int B,H;
	static boolean flag = false;
	static{
		Scanner sc = new Scanner(System.in);
		B = sc.nextInt();
		H = sc.nextInt();
		if (B<0 || H<0) System.out.println("Loi");
		else flag = true;
	}

	public static void main(String[] args){
		if(flag){
			int area=B*H;
			System.out.print(area);
		}
		
	}
        
}
