package javavabasic;

import java.util.Scanner;

public class A {
	private static Scanner scanner = new Scanner(System.in);
	
	public static int UCLN(int a, int b) {
		return (b == 0) ? a: UCLN(b, a % b);
	}
	
	public static int BCNN(int a, int b) {
		if (UCLN(a, b)>0) {
			return (a*b)/(UCLN(a, b));
		}
		else {
			return -(a*b)/(UCLN(a, b));
		}
	}
	
	public static void main(String[] args) {
		int n = scanner.nextInt();
		int counter =0;
		int[] arr = new int[n*2];
		int index =0;
		int maxVal = (int) (Math.pow(10, 9)-1);
		
		while (counter <n) {
			int a, b;
			do {
				a = scanner.nextInt();
			} while (a >maxVal);
				
			do {
				b = scanner.nextInt();
			} while (b >maxVal);
			
			arr[index++] = a;
			arr[index++] = b;
			counter++;
		}
		
		for (int i=0; i <arr.length-1; i+=2) {
			System.out.println(UCLN(arr[i], arr[i+1]) + " " + BCNN(arr[i], arr[i+1]));
		}
	}
}
